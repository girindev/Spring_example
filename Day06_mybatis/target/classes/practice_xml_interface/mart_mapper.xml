<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="practice_xml_interface.MartMapper">
	<insert id="insert" parameterType="MartVO">
		INSERT into mart(name, price, type, count)
		VALUES(#{name}, #{price}, #{type}, #{count})
	</insert>

	<select id="selectMart" resultType="MartVO">

	</select>
	<select id="selectList" resultType="MartVO">
		SELECT * FROM Mart
	</select>
</mapper>